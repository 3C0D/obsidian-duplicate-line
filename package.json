	"license": "MIT",
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/lodash": "^4.14.194",
		"@types/node": "^16.11.6",
		"@types/semver": "^7.5.8",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"cross-env": "^7.0.3",
		"dedent": "^1.5.1",
		"dotenv": "^16.4.5",
		"fs-extra": "^11.2.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"lodash": "^4.17.21",
		"obsidian": "latest",
		"obsidian-typings": "latest",
		"semver": "^7.6.0",
		"tslib": "2.4.0",
		"tsx": "^4.19.4",
		"typescript": "^5.8.2",
		"eslint": "^9.0.0"
	},
	"dependencies": {
		"obsidian-plugin-config": "file:../obsidian-plugin-config"
	},
	"engines": {
		"npm": "please-use-yarn",
